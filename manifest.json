{
	"manifest_version": 2,
	"name": "Flashcard Dictionary",
	"version": "0.1",
	"default_locale": "en",
	"description": "This is a Chrome extension that enables highlight to search dictionary functionality while saving the searched word into flash cards.",
	"icons": {
		"16": "images/dictionary-logo-16.png",
		"32": "images/dictionary-logo-32.png",
		"48": "images/dictionary-logo-48.png",
		"64": "images/dictionary-logo-64.png",
		"72": "images/dictionary-logo-72.png",
		"96": "images/dictionary-logo-96.png",
		"120": "images/dictionary-logo-120.png",
		"240": "images/dictionary-logo-240.png"
	},
	"browser_action": {
		"default_icon": {
			"16": "images/dictionary-logo-16.png",
			"32": "images/dictionary-logo-32.png",
			"48": "images/dictionary-logo-48.png",
			"64": "images/dictionary-logo-64.png",
			"72": "images/dictionary-logo-72.png",
			"96": "images/dictionary-logo-96.png",
			"120": "images/dictionary-logo-120.png",
			"240": "images/dictionary-logo-240.png"		
		},
		"default_title": "Flashcard Dictionary",
		"default_popup": "popup.html"
	}
}